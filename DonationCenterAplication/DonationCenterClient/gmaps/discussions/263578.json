</span>\r\n        <span style=\"color: blue;\">public</span> Image MarkerImage\r\n        {\r\n            <span style=\"color: blue;\">get</span>\r\n            {\r\n                <span style=\"color: blue;\">return</span> img;\r\n            }\r\n            <span style=\"color: blue;\">set</span>\r\n            {\r\n                img = value;\r\n            }\r\n        }\r\n\r\n        <span style=\"color: gray;\">///</span> <span style=\"color: gray;\">&lt;summary&gt;</span>\r\n        <span style=\"color: gray;\">///</span><span style=\"color: green;\"> Constructor</span>\r\n        <span style=\"color: gray;\">///</span> <span style=\"color: gray;\">&lt;/summary&gt;</span>\r\n        <span style=\"color: gray;\">///</span> <span style=\"color: gray;\">&lt;param name=\"p\"&gt;</span><span style=\"color: green;\">The position of the marker&lt;/param&gt;</span>\r\n        <span style=\"color: blue;\">public</span> GMapMarkerImage(PointLatLng p, Image image)\r\n            : <span style=\"color: blue;\">base</span>(p)\r\n        {\r\n            img = image;\r\n            Size = img.Size;\r\n            Offset = <span style=\"color: blue;\">new</span> System.Drawing.Point(-Size.Width / 2, -Size.Height / 2);\r\n        }\r\n\r\n        <span style=\"color: blue;\">public</span> <span style=\"color: blue;\">override</span> <span style=\"color: blue;\">void</span> OnRender(Graphics g)\r\n        {\r\n            g.DrawImage(img, LocalPosition.X, LocalPosition.Y, Size.Width, Size.Height);\r\n        }\r\n    }\r\n}\r\n</pre>\r\n</div>\r\n<p>&nbsp;</p>\r\n</blockquote>\r\n<p>&nbsp;</p>",
